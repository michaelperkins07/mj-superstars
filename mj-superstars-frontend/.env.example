# ============================================================
# MJ's Superstars Frontend - Environment Variables
# Copy this file to .env.local and fill in your values
# ============================================================

# API Configuration
REACT_APP_API_URL=http://localhost:3001
REACT_APP_WS_URL=ws://localhost:3001

# Analytics
REACT_APP_MIXPANEL_TOKEN=your-mixpanel-token

# App Info
REACT_APP_VERSION=$npm_package_version
REACT_APP_BUILD_DATE=

# Feature Flags
REACT_APP_ENABLE_WATCH=true
REACT_APP_ENABLE_HEALTHKIT=true
REACT_APP_ENABLE_BUDDY_SYSTEM=true

# Error Tracking
REACT_APP_SENTRY_DSN=

# Debug
REACT_APP_DEBUG=false
